{
  "name": "imsprototype",
  "version": "1.0.0",
  "description": "Inventory management system prototype",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --port 3006 --content-base src/ --inline --history-api-fallback",
